public class wsHelper {
	
public int findCluster(HashMap<Integer, ArrayList<String>> arrayLists, String word)
{
	int cluster=0;
	Set<Integer> keys = arrayLists.keySet();
	for(int i : keys)
	{
		for(String s : arrayLists.get(i))
		{
			if(s.equals(word))
			{
				return i;
			}
		}
	}
	
	return cluster;
	
}
